<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>testes_deploy</display-name>
  <welcome-file-list>    
    <welcome-file>index.jsp</welcome-file>    
  </welcome-file-list>
  
  <servlet>
  	<servlet-name>Login</servlet-name>
  	<servlet-class>controller.LoginController</servlet-class>  	
  </servlet>
  
  <servlet>
  	<servlet-name>Contatos</servlet-name>
  	<servlet-class>controller.ContatosController</servlet-class>
  </servlet>
  
  <servlet-mapping>
  		<servlet-name>Contatos</servlet-name>
  		<url-pattern>/home</url-pattern>
  		<url-pattern>/listar</url-pattern>
  		<url-pattern>/cadastrar</url-pattern>
  		<url-pattern>/excluir</url-pattern>  
  </servlet-mapping>
    
</web-app>